<p id="notice"><%= notice %></p>

<h1>Listing Feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Bind</th>
      <th>Responce click</th>
      <th>Responce close</th>
      <th>Time click</th>
      <th>Time close</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.bind_id %></td>
        <td><%= feedback.responce_click %></td>
        <td><%= feedback.responce_close %></td>
        <td><%= feedback.time_click %></td>
        <td><%= feedback.time_close %></td>
        <td><%= link_to 'Show', feedback %></td>
        <td><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
        <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feedback', new_feedback_path %>
